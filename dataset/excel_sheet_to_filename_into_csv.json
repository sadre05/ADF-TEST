{
	"name": "excel_sheet_to_filename_into_csv",
	"properties": {
		"linkedServiceName": {
			"referenceName": "linekedwithSource",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"fileName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().fileName",
					"type": "Expression"
				},
				"folderPath": "output",
				"fileSystem": "sink"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "name",
				"type": "String"
			},
			{
				"name": "gender",
				"type": "String"
			},
			{
				"name": "department",
				"type": "String"
			}
		]
	}
}